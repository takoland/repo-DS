    "#read files of happiness data and economy level \n",
    "raw_economy_df = pd.read_csv(\"Economy_Level_dataset.csv\")\n",
    "raw_happiness_df = pd.read_csv(\"Happiness_dataset_cleaned.csv\")\n",
    "\n",
    "dropped_column_hapiness_df = raw_happiness_df.drop(columns=['Unnamed: 0','Confidence in national government','Democratic Quality','Delivery Quality','Standard deviation of ladder by country-year','Standard deviation/Mean of ladder by country-year','GINI index (World Bank estimate)','GINI index (World Bank estimate), average 2000-15','gini of household income reported in Gallup, by wp5-year'])\n",
    "dropped_column_economy_df = raw_economy_df.drop(columns=['Unnamed: 0'])\n",
    "renamed_happiness_df = dropped_column_hapiness_df.rename(columns={'country':'Country'})\n",
    "merge_happiness_economy_df = pd.merge(renamed_happiness_df, dropped_column_economy_df, on='Country', how='inner')\n",
    "renamed_happiness_economy_df = merge_happiness_economy_df.rename(columns={'Country':'country','Economy level':'Economy_Level'})\n",
    "\n",
    "renamed_happiness_economy_df.to_csv(\"Happiness_with_Economy_dataset.csv\")"
